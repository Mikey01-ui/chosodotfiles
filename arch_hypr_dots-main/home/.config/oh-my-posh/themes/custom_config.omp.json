{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "> ",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "foreground": "#f8f8f2",
          "template": "\n╭─ 󰉖 {{ path .Path .Location }}",
          "type": "path"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [

        ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "╰─ >",
          "foreground": "#f8f8f2",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "patch_pwsh_bleed": true,
  "final_space": true
}
